{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:production": "NODE_ENV=production vite build",
    "build:analyze": "vite build --config vite.config.analyze.js",
    "build:verify": "npm run build && node scripts/verify-build.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "postbuild": "echo 'Frontend build completed successfully'",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:build": "npm run build && npm run preview --port 4173 &",
    "validate": "npm run lint && npm run build:verify"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^3.0.1",
    "@headlessui/react": "^2.2.7",
    "@react-three/drei": "^10.6.1",
    "@react-three/fiber": "^9.3.0",
    "@tailwindcss/vite": "^4.1.11",
    "axios": "^1.11.0",
    "framer-motion": "^12.23.12",
    "googleapis": "^156.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.8.0",
    "tailwindcss": "^4.1.11",
    "three": "^0.179.1",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^23.0.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "vite": "^7.1.2",
    "vitest": "^1.0.4"
  }
}
